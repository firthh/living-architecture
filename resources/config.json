{
    "metric": {
        "avg_http_response_time": {
            "name": "avg_http_response_time",
            "unit": "ms",
            "source": "prometheus",
            "host": "http://demo.robustperception.io:9090/",
            "query": "avg(avg without (quantile)(rate(http_request_duration_microseconds[5m]) >= 0))"
        },
        "sum_http_requests_handled": {
            "name": "sum_http_requests_handled",
            "unit": "requests",
            "source": "prometheus",
            "host": "http://demo.robustperception.io:9090/",
            "query": "sum(http_requests_total)"
        },
        "sum_bytes_sent": {
            "name": "sum_bytes_sent",
            "unit": "bytes",
            "source": "prometheus",
            "host": "http://demo.robustperception.io:9090/",
            "query": "sum(http_response_size_bytes_sum)"
        }
    },
