{
    "diagram": [{
            "name": "Load Balancer 1",
            "id": "lb1",
            "position": {
                "x": 100,
                "y": 100
            },
            "metrics": [{
                    "id": "met1",
                    "name": "rps",
                    "value": 50
                },
                {
                    "id": "met2",
                    "name": "latency",
                    "value": 100
                }
            ]
        },
        {
            "name": "Load Balancer 2",
            "id": "lb2",
            "position": {
                "x": 500,
                "y": 100
            },
            "metrics": [{
                    "id": "met3",
                    "name": "rps",
                    "value": 30
                },
                {
                    "id": "met4",
                    "name": "latency",
                    "value": 150
                }
            ]
        }
    ],
    "metric": {
        "avg_http_response_time": {
            "name": "avg_http_response_time",
            "unit": "ms",
            "source": "prometheus",
            "host": "http://demo.robustperception.io:9090/",
            "query": "avg(avg without (quantile)(rate(http_request_duration_microseconds[5m]) >= 0))"
        },
        "sum_http_requests_handled": {
            "name": "sum_http_requests_handled",
            "unit": "requests",
            "source": "prometheus",
            "host": "http://demo.robustperception.io:9090/",
            "query": "sum(http_requests_total)"
        },
        "sum_bytes_sent": {
            "name": "sum_bytes_sent",
            "unit": "bytes",
            "source": "prometheus",
            "host": "http://demo.robustperception.io:9090/",
            "query": "sum(http_response_size_bytes_sum)"
        }
    }
}
